        $(document).ready(function () {

            var enabledOnMobile = 'False' === 'True';

            var width = $('.popup-pnl').eq(0).width();

            // dont show popups on devices if it can't fit on the screen
            var isMobile = window.innerWidth <= width + 12 || window.innerWidth < 1025;

            // ignore cookies for preview
            var isPreview = 'False' === 'True';

            if(isMobile && !enabledOnMobile)
                return;

            if ((typeof $.cookie('fdn-popup_17') === 'undefined' || isPreview)) {

                if(!isMobile || enabledOnMobile)
                    initializePopUp(isMobile);

            }

            Sys.WebForms.PageRequestManager.getInstance().add_endRequest(endRequestHandler);

        });


        function endRequestHandler(sender, args)
        {
            setPopupCookie();

            TrackEvent('Email Signup','Popup','Sign up for email',true);

            setTimeout(function(){ 
                $('.js-popup').fadeOut().overlay().close();
            }, 3500);
        }


        function setPopupCookie(){

            $.cookie('fdn-popup_17', '1', { expires: 30 });

        }

        
        function initializePopUp(isMobile) {

            if (isMobile) {

                $('.popup__mobile-html .popup__mobile-email').prepend($('.popup .popup__form').detach().show());

                $('.popup__mobile-html .popup__mobile-email .emailaddress-textbox').after($('.popup__mobile-html .popup__mobile-btn').detach().show());

                var delayTime = parseInt(0) * 1000;

                setTimeout(function(){
                    $('.popup__mobile-html, .popup-pnl.js-popup').css('display', 'block');
                    $('.popup__mobile-html').animate({bottom:'0'}, 2500);
                    $('.popup__rv-hp').val('_rvhp_');
                    setPopupCookie();
                }, delayTime);

                $('.popup__mobile-html .fdnicon-close').on('click',function(){
                    $('.popup__mobile-html').animate({bottom:'-100em'}, 2500);
                });

            } else {

                $('#popup-form-placeholder').append($('.popup .popup__form').detach().show());

                $('.popup__desktop-html').css('display', 'block');

                $('.js-popup').overlay({
                    closeOnClick: true,
                    fixed: true,
                    load: false,
                    mask: { color: '#ccc', loadSpeed: 200, opacity: 0.6 },
                    top: '15%',
                    left:'center',
                    speed: 'fast',
                    onClose: function () {
                        setPopupCookie();
                    }
                });

                var delayTime = parseInt(0) * 1000;

                setTimeout(function(){
                    $('.js-popup').overlay().load();
                    $('.popup__rv-hp').val('_rvhp_');
                    setPopupCookie();
                }, delayTime);

                $('.js-popup-close').on('click',function(){
                    $('.js-popup').overlay().close();
                });

            }

        }

    </script>


