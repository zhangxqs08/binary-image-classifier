                <div class="l-col-xl-6 l-col-md-12 l-col-sm-6 l-col-xs-12"><div class="email-only-reg EmailOnlyReg">

    <label class="email-only-reg__form__label">Your Email Address</label>
    <input class="email-only-reg__form__textbox submithook" id="EmailAddress" name="EmailAddress" placeholder="Your Email Address" type="text" value="" />
    

    <input type="button" value="Submit" id="savebtn" class="email-only-reg__form__submit l-row--mar-sm no-mar-top" role="button" onclick="EmailOnlyRegisterClick(this); return false;" />

    <span class="email-only-reg__status"></span>

    <div class="xexyz honey">
        <label class="fdn-form-label" for="xexyz">Name - Required*</label>
        <input class="form-control requiredname fdn-form-textbox" id="xexyz" name="xexyz" type="text" value="" />
        <label for="xexyz_cb">Do you agree to the terms of service?</label>
        <input class="terms-cb" id="xexyz_cb" name="xexyz_cb" type="checkbox" value="true" /><input name="xexyz_cb" type="hidden" value="false" />
    </div>

</div>

<script>
    //make enter button enter submit user/pass
    //little tricky had to use keypress event instead of keyup since the formsubmits before keyup can intercept and preventDefault()
    $('.submithook').keypress(function (event) {
        if (event.keyCode == 13) {
            event.preventDefault();
            $("#savebtn").click();
            return false;
        }
    });

    $('.EmailOnlyReg .email-only-reg__form__textbox').on('focusout', function () {
        validateRegEmailAddress();
    });

    function EmailOnlyRegisterClick(registerBtn) {
        //use spinner here
        FormButtonSpinner(registerBtn, 'EmailOnlyReg');

        var parent = $(registerBtn).parent('.email-only-reg');
        var email = $(parent).children('.email-only-reg__form__textbox').val().trim();

        //validate the page
        var pageValid = true;

        if (email.length == 0) {
            setRegStatusMessage('Please enter your email address first.');
            pageValid = false;
        }

        pageValid = validateRegEmailAddress();

        if (!pageValid) {
            FormButtonSpinnerRemove(registerBtn);
            return false;
        }

        //create regvm (simpleregistrationviewmodel)
        var regvm = {};
        regvm.EmailAddress = email;
        regvm.xexyz = $(parent).find('.requiredname').val();
        regvm.xexyz_cb = $(parent).find('.terms-cb').prop('checked');

        registration.EmailOnlyRegister(regvm);

        return false;
    }

    function setRegStatusMessage(text) {
        $('.EmailOnlyReg .email-only-reg__status').html(text);
    }

    function validateRegEmailAddress() {
        var email = $('.EmailOnlyReg .email-only-reg__form__textbox').val().trim();

        if (email.length > 0) {
            if (!Utils.validateEmail(email)) {
                setRegStatusMessage('Please enter a valid email address.');
                return false;
            } else {
                setRegStatusMessage('');
                return true;
            }
        }
    }

    function errorCallback(status)
    {
        FormButtonSpinnerRemove();

        switch (status) {
            case 2:
                setRegStatusMessage("We're sorry. There was an error processing your registration. Please try again.");
                break;

            default:
                setRegStatusMessage("We're sorry. There was an error processing your registration. Please try again.");
                break;

        }
    }
</script></div>
