        // toggle menu visibility
        (function(clicker){
            if(!clicker) return;
            var menuId = clicker.getAttribute('aria-controls');
            var breakpoint = clicker.getAttribute('data-breakpoint');
            var menu = doc.getElementById(menuId);
            var is_expanded = window.innerWidth > breakpoint || window.innerHeight > breakpoint; //window.matchMedia("(orientation: portrait)").matches
            var subClickers = menu.querySelectorAll('.js-submenu-expander');
            var openClicker = null;
            enableSubmenus(subClickers);
            // initial setup
            clicker.setAttribute('aria-expanded', Reyez.b2s(is_expanded));
            menu.setAttribute('aria-hidden', Reyez.b2s(!is_expanded));

            function enableSubmenus(clickers){
                Reyez.loop(clickers, function _enableSubMenus(clicker) {
                    var submenu = document.getElementById(clicker.getAttribute('aria-controls'));
                    clicker.addEventListener('click', function _subClick(){
                        var is_expanded = clicker.getAttribute('aria-expanded') === 'false';
                        submenu.setAttribute('aria-hidden', Reyez.b2s(!is_expanded));
                        clicker.setAttribute('aria-expanded', Reyez.b2s(is_expanded));
                        if(openClicker === clicker) openClicker = null;
                        if(openClicker && openClicker !== clicker) openClicker.click();
                        if(is_expanded) openClicker = clicker;
                    });
                });
            }
            function showMenu(show){
                menu.setAttribute('aria-hidden', Reyez.b2s(!show));
                clicker.setAttribute('aria-expanded', Reyez.b2s(!is_expanded));
                is_expanded = show;
            }

            function recalculate (){
                if(window.innerWidth < breakpoint) { // window is so small enough for menu to collapse
                    if(is_expanded) showMenu(false);
                }
                else { //window is to big - menu stays visible - clicker is not visible
                    if(!is_expanded)  showMenu(true);
                }
            }

            clicker.addEventListener('click', function _click(){
                showMenu(!is_expanded);
            });

            menu.addEventListener('click', function _click(e){
                if( ! (e.target.tagName === 'A')) return;
                recalculate();
            });

            Reyez.pubsub.subscribe('hideMenuBar', function(){
                if(openClicker) openClicker.click();
            });

            window.addEventListener('resize', recalculate, false);
            setTimeout(recalculate, 100);
        })(document.getElementById('mainMenuButton'));

        //toggle search bar
        (function(clicker){
            var target = document.getElementById(clicker.getAttribute('aria-controls'));
            var focusEl = document.querySelector(clicker.getAttribute('data-focus-el-csspath'));
            var expanded = clicker.getAttribute('aria-expanded') === 'true';
            target.setAttribute('aria-hidden', !expanded);
            clicker.addEventListener('click', function _click(){
                clicker.setAttribute('aria-expanded', !expanded);
                target.setAttribute('aria-hidden', expanded);
                expanded = !expanded;
                if(expanded === true) focusEl.focus();
            });

        })(document.querySelector('.js-search-bar-expander'));

        //toggle div
        (function(selector){
            Reyez.pubsub.subscribe('productFilterHtml/refresh', init);

            init();

            function init(){
                var showlists = document.querySelectorAll(selector);
                Reyez.loop(showlists, function(showlist) {
                    showlist.addEventListener('click', function _click(){
                        var newClass = 'is-shown';
                        var clickClass = 'is-clicked';
                        var myList = showlist.getAttribute('data-toggle');
                        var getList = document.querySelector('[data-togglechild="'+ myList +'"]');
                        var getListClass = getList.className;
                        if (getListClass.indexOf(newClass) !== -1) {
                            getList.className = getListClass.replace(newClass, '');
                            this.className = this.className.replace(clickClass,'');

                        } else {
                            getList.className += ' '+ newClass;
                            this.className += ' '+ clickClass;
                        }
                    });
                });
            }


        })('.toggle-parent');

        (function(){
            Reyez.pubsub.subscribe('filterRefresh', hideSubmits);

            hideSubmits();

            function hideSubmits(){
                var forms = document.querySelectorAll('.js-perpage-submit');
                if(!forms.length) return;
                Reyez.loop(forms, function(formPerPage) {
                    var formSubmitClass = 'is-visually-hidden';
                    var formSubmits = formPerPage.querySelectorAll('[type=submit]');
                    Reyez.loop(formSubmits, function(formSubmit) {
                        Reyez.cssClass.add(formSubmit, formSubmitClass);
                    });
                    formPerPage.addEventListener('change', function() {
                        formPerPage.submit();
                    });
                });
            }
        })();

        // Aanbod filters
        (function(form){
            if(!form) return;

            var filterContainer = document.getElementById(form.getAttribute('data-filter-container-id'));
            var productContainer = document.getElementById(form.getAttribute('data-products-container-id'));
            var paginatorContainer = document.getElementsByClassName(form.getAttribute('data-pagination-container-class'));
            var totalProducts = document.getElementById(form.getAttribute('data-total-count-id'));

            var urlPart = location.pathname;
            var els = form.querySelectorAll('[type=checkbox]');
            var hasProgressLoaderEls = document.querySelectorAll('.js-has-progress-loader');
            var progressClass = ' in-progress';

            window.addEventListener('popstate', onHistory);

            form.addEventListener('change', Reyez.debounce(function(e){
                onChange(e);
                addLoader();
                closeFilter();
            }, 1000));

            function closeFilter(){

                Reyez.loop(document.getElementsByClassName('filter-list'),function (list) {
                    Reyez.cssClass.remove(list, 'is-shown');
                });
            }

            function addLoader(){
                Reyez.loop(hasProgressLoaderEls, function(hasProgressLoaderEl) {
                    Reyez.cssClass.add(hasProgressLoaderEl, progressClass);
                });
            }

            function onSuccess(data){
                history.pushState('', '', this.responseURL ? this.responseURL.replace(/[?&]ajax=1/, '') : this.responseURL);

                productContainer.innerHTML = data.productsHtml;
                totalProducts.innerHTML = data.total_products_text;

                Reyez.loop(paginatorContainer, function(selectedPaginator) {
                    selectedPaginator.innerHTML = data.paginatorHtml;
                });

                Reyez.loop(hasProgressLoaderEls, function(hasProgressLoaderEl) {
                    Reyez.cssClass.remove(hasProgressLoaderEl, progressClass);
                });

                Reyez.pubsub.publish('filterRefresh');
            }

            function onError(data){
                console.warn(data);
            }

            function onHistory(e) {
                function historySucces(data){
                    filterContainer.innerHTML = data.productFilterHtml;
                    Reyez.pubsub.publish('productFilterHtml/refresh');
                    onSuccess.call({responseURL: location.href}, data);
                }

                doRequest({
                    url: location.search,
                    onSuccess: historySucces,
                    onError: onError
                });
            }

            function onChange(e){
                e.stopPropagation();
                var checkedEls = getCheckedInputs();
                var getString = makeGetString(checkedEls);

                getString = getString ? '?' + getString : '';

                var perPage = location.search.match(/per=([0-9]+)/);
                if(perPage && perPage.length){
                    var perPageNum = perPage[1];
                    getString += '&per='+perPageNum;
                }

                doRequest({
                    url: urlPart + getString,
                    onSuccess: onSuccess,
                    onError: onError
                });
            }

            function getCheckedInputs(){
                return [].filter.call(els, function(el){
                    return el.checked;
                });
            }
            function makeGetString(checkedEls){
                return checkedEls.map(function(el){
                    return el.name + '=' + el.value;
                }).join('&');
            }
            function doRequest(options){
                var request = new XMLHttpRequest();
                var url;
                if(options.url.indexOf('?') > -1){
                    url = options.url+'&ajax=1';
                }
                else{
                    url = location.pathname+'?ajax=1';
                }
                request.open('GET', url , true);
                request.setRequestHeader('X-Requested-With', 'XMLHttpRequest');
                request.onload = function() {
                    if (this.status >= 200 && this.status < 400) {
                        options.onSuccess.call(this, JSON.parse(this.response));
                    } else {
                        options.onError.call(this, this.status);
                    }
                };
                request.onerror = function(error) {
                    options.onError.call(this, error);
                };
                request.send();

            }

        })(document.getElementById('product-filter'));

        (function clickOnFilterLabelToExpandSubFilters(selector){
            var clickers = doc.querySelectorAll(selector);
            if(!clickers.length) return;

            Reyez.pubsub.subscribe('productFilterHtml/refresh', init);
            init();
            function init(){
                clickers = doc.querySelectorAll(selector);
                Reyez.loop(clickers, function _initClicker(clicker){
                    var cssPath = clicker.getAttribute('data-children-via-parent');
                    var clickerCheckbox = clicker.previousElementSibling;
                    if(cssPath) {
                        var children = clicker.parentNode.querySelectorAll(cssPath);

                        if(!allChecked(children) && !allUnChecked(children)){
                            // initial state. indeterminate cant be set through html
                            clickerCheckbox.checked = false;
                            clickerCheckbox.indeterminate = true;
                        }

                        children[0].parentNode.parentNode.addEventListener('click', function(){
                            if(allChecked(children)){
                                clickerCheckbox.checked = true;
                                clickerCheckbox.indeterminate = false;
                            }
                            else {
                                if(allUnChecked(children)){
                                    clickerCheckbox.checked = clickerCheckbox.indeterminate = false
                                }
                                else{
                                    clickerCheckbox.checked = false;
                                    clickerCheckbox.indeterminate = true;
                                }
                            }
                        });
                    }

                    clicker.addEventListener('click', function(){
                        if(clickerCheckbox.checked && Reyez.cssClass.contains(clicker.nextElementSibling, 'is-clicked') ||
                            !clickerCheckbox.checked && !Reyez.cssClass.contains(clicker.nextElementSibling, 'is-clicked')) {
                            clicker.nextElementSibling.click(); // click the expand/collapse btn
                        }
                        if(children) copyStateOfParent(clickerCheckbox, children);
                    });
                });
            }

            function allChecked(items){
                return Reyez.every(items, function(item){
                    return item.checked === true;
                });
            }
            function allUnChecked(items){
                return Reyez.every(items, function(item){
                    return item.checked === false;
                });
            }

            function copyStateOfParent(parent, children){
                setTimeout(function(){
                    Reyez.loop(children, function (child) {
                        if(child.checked === parent.checked) return;
                        child.checked = parent.checked;
                        child[(parent.checked?'set':'remove')+'Attribute']('checked', 'checked');
                    });
                }, 0); // overcome the small delay that exists between clicking on the <label> and checking the checkbox
            }
        })('.js-show-subfilters');

        //tabs/carousel
        (function(tablists){
            Reyez.loop(tablists, function initTabs(tablist){
                var tabs = tablist.querySelectorAll('[role=tab]');
                if(!tabs.length) return;
                var selectedTab = Reyez.filter(tabs, function(tab){ return tab.getAttribute('aria-selected') === 'true'})[0];
                var tab_panel_map = [];
                var panel_tab_map = [];
                var selectedTabCssClass = tablist.getAttribute('data-selected-tab-cssclass');
                var autoPlayTimeOut = Number(tablist.getAttribute('data-carousel-autoplay'));
                var autoPlayTimer = null;

                // event handlers
                function switch_active_panel(e){
                    clearTimeout(autoPlayTimer);
                    e && e.preventDefault && e.preventDefault();
                    if(window.innerWidth < 768) {
                        window.location.href = this.getAttribute('data-url');
                    } else {
                        select.call(selectedTab, false);
                        select.call(this, true);
                        selectedTab = this;
                    }
                    autoPlayTimer = setTimeout(autoPlay, autoPlayTimeOut)
                }
                function switch_active_tab(e){
                    var key = e.key.toLowerCase();
                    if( -1 === Reyez.array.indexOf.call(['left', 'right'], key)) return;
                    var currentIndex = Reyez.array.indexOf.call(tabs, selectedTab);
                    var newIndex = currentIndex + (key === 'left' ? -1:1);
                    // make sure index is within bounds
                    newIndex = Math.max(Math.min(newIndex, tabs.length - 1), 0);
                    if(newIndex !== currentIndex){
                        switch_active_panel.call(tabs[newIndex], e);
                    }
                }
                // select or unselect
                function select(shouldSelect){
                    var index = Reyez.array.indexOf.call(tabs, this);
                    this.setAttribute('aria-selected', Reyez.b2s(shouldSelect));
                    Reyez.cssClass[shouldSelect ? 'add':'remove'](tab_panel_map[index], selectedTabCssClass);
                }

                Reyez.loop(tabs, function _couple(tab, i){
                    tab.addEventListener('focus', switch_active_panel);
                    tab.addEventListener('click', switch_active_panel);
                    tab.addEventListener('keydown', switch_active_tab);
                    tab_panel_map[i] = document.getElementById(tab.getAttribute('aria-controls'));
                    panel_tab_map[i] = tab;
                });

                function autoPlay(){
                    var newIndex = Reyez.array.indexOf.call(tabs, selectedTab) + 1;
                    switch_active_panel.call(tabs[(newIndex > tabs.length - 1) ? 0:newIndex])
                }

                if(tablist.getAttribute('data-carousel-autoplay')){
                    autoPlayTimer = setTimeout(autoPlay, autoPlayTimeOut)
                }
            });
        })(document.querySelectorAll('[role=tablist]'));

        // change background of slider
        (function(containers) {
            Reyez.loop(containers, function(container) {
                var target = document.getElementById(container.getAttribute('data-change-bg-target'));
                var cssClass = container.getAttribute('data-change-bg-clicker-class');
                var cssPrefix = container.getAttribute('data-change-bg-prefix');
                var regex = new RegExp('(^| )' + cssPrefix + '[a-z\-]+( |$)', 'gi');
                target.addEventListener('click', function(e) {
                    if( ! (Reyez.cssClass.contains(e.target, cssClass) || Reyez.cssClass.contains(e.target.parentNode, cssClass))) return;
                    var el = Reyez.cssClass.contains(e.target.parentNode, cssClass) ? e.target.parentNode:e.target;
                    var cssClassPart = el.getAttribute('data-cssclass');
                    target.setAttribute('class', target.getAttribute('class').replace(regex, ' '+ cssPrefix + cssClassPart +' '));
                });
            });
        })(document.querySelectorAll('.js-change-bg'));

        // animated scrolling
        (function(wrapper, animatingClass, getHash, href, offset, isAnimating){
            if(!wrapper) return;
            function onTransitionEnd(){
                if(!isAnimating) return;
                Reyez.cssClass.remove(wrapper, animatingClass);
                wrapper.style.transform = '';
                window.scrollTo(0, Math.abs(offset));
                if(href!=='') window.location = href;
                href = '';
                isAnimating = false;
            }
            function scrollToAnchor(e){
                if( ! Reyez.hasTransitionEndSupport) return;
                var target = (e.target.tagName === 'A' ? e.target : null) || (this.tagName === 'A' ? this : null);
                if( ! target) return;
                var matches = target.href.match(getHash);
                if( ! (matches && matches.length)) return;
                href = matches[0];
                var el = document.getElementById(href.replace('#', ''));
                if(!el) return;
                e.preventDefault();
                offset = el.offsetTop;
                var scroll = Reyez.getScroll().y;
                var amount = Math.abs(offset - scroll);
                if( ! (scroll === amount)){
                    var scrollspace = Math.abs(window.innerHeight - wrapper.clientHeight);
                    amount = Math.min(offset, scrollspace) - scroll;
                    amount = (amount > scrollspace) ? -(scrollspace - scroll) : -amount;
                }
                Reyez.cssClass.add(wrapper, animatingClass);
                isAnimating = true;
                var transform = 'translate(0, ' + amount + 'px)';
                wrapper.style.webkitTransform = transform;
                wrapper.style.transform = transform;
            }
            if(Reyez.hasTransitionEndSupport) wrapper.addEventListener(Reyez.hasTransitionEndSupport, onTransitionEnd);
            Reyez.loop(doc.querySelectorAll('.js-scrollto'), function(el){
                if(el) el.addEventListener('click', scrollToAnchor);
            });
        })(doc.querySelector('.js-scroll-area'), 'is-animating', /#[a-zA-Z0-9\-_]+/, '', 0, false);

        // show header when scrolling up
        (function(header){
            if(!header) return;
            var mayanimateClass = header.getAttribute('data-mayanimate-class');
            var hideClass = header.getAttribute('data-hide-class');
            var floatingClass = header.getAttribute('data-floating-class');
            var breakpoint = header.getAttribute('data-breakpoint');
            var scrollTop = Reyez.getScroll().y;
            var lastScrollTop = scrollTop;
            var isHiding = false;
            var thresshold = 100;
            var thressholdOffset = 50;
            var headerHeight;
            var windowInnerWidth;

            measure();

            window.addEventListener('resize', Reyez.debounce(function(){ measure(); }));
            window.addEventListener('orientationchange', function(){ measure(); });

            window.addEventListener('scroll', onScroll);

            function measure(){
                headerHeight = header.clientHeight;
                windowInnerWidth = window.innerWidth;
                thressholdOffset = (windowInnerWidth > breakpoint) ? 50:0;
            }
            function onScroll(){
                scrollTop = Reyez.getScroll().y;
                if(scrollTop >= (thresshold + thressholdOffset)) {// not @ top portion of page
                    if(!isHiding){                                  // not hidden
                        if(lastScrollTop < scrollTop){                  // we are going down
                            Reyez.cssClass.add(header, hideClass);
                            Reyez.cssClass.add(header, floatingClass);
                            isHiding = true;
                            Reyez.pubsub.publish('hideMenuBar');
                            setTimeout(function(){
                                Reyez.cssClass.add(header, mayanimateClass);
                            }, 100);
                        }
                    } else {                                        // hidden
                        if(lastScrollTop > scrollTop){                  // we are going up
                            if(isHiding) {
                                Reyez.cssClass.remove(header, hideClass);
                                isHiding = false;
                            }
                        }
                        else {                                          // we are going down
                            if(!isHiding) {
                                Reyez.cssClass.add(header, hideClass);
                                isHiding = true;
                                Reyez.pubsub.publish('hideMenuBar');
                            }
                        }
                    }
                }
                else {
                    if(scrollTop <= thressholdOffset) {
                        Reyez.cssClass.remove(header, hideClass);
                        Reyez.cssClass.remove(header, floatingClass);
                        Reyez.cssClass.remove(header, mayanimateClass);
                        isHiding = false;
                    }
                }
                lastScrollTop = scrollTop;
            }
        })(document.querySelector('.header'));

        (function(switchers){
            if(!switchers.length) return;
            Reyez.loop(switchers, function(switcher){
                var id = switcher.getAttribute('data-image-switcher-target');
                var activeCssClass = switcher.getAttribute('data-image-switcher-active-class');
                var target = document.getElementById(id);
                var activeEl = document.querySelector('.'+activeCssClass);

                switcher.addEventListener('click', function _click(e){
                    e.preventDefault();
                    var target = e.target.tagName === 'A' ? e.target:e.target.parentNode;
                    if(target) doSwitch(target);
                });

                function doSwitch(el){
                    Reyez.cssClass.add(el, activeCssClass);
                    Reyez.cssClass.remove(activeEl, activeCssClass);
                    activeEl = el;
                    target.src = el.getAttribute('data-big-image-url');
                }
            });
        })(document.querySelectorAll('.js-image-switcher'));

        (function(inputs){
            if(!inputs.length) return;
            Reyez.loop(inputs, function(input){
                var label = input.nextElementSibling;
                var labelVal = label.innerHTML;
                var fileName = '';
                input.addEventListener( 'change', function(e){
                    fileName = e.target.value.split('\\').pop();
                    label.innerHTML = fileName ? fileName : labelVal;
                });
            });
        })(document.querySelectorAll('.form-uploader'));

        (function _carouselClickCapturer(containers){
            Reyez.loop(containers, function(container){
                var a = container.querySelector(container.getAttribute('data-click-capturer-target'));
                container.addEventListener('click', function(){
                    window.location = a.href;
                });
            });
        })(doc.querySelectorAll('.js-carousel-click-capturer'));
    })(window, document);
</script>
</body>
</html>
