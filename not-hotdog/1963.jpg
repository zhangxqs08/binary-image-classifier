<script>
    (function(win, doc, undefined){
        if(! (doc.querySelector && doc.addEventListener)) return;
        'use strict';
        window.Reyez = {};
        (function(ns){
            ns.cssClass = {
                _classRegex: function(className){
                    return new RegExp('(^| )' + className + '( |$)', 'gi');
                },
                contains: function containsCSSClass(el, className){
                    var currentClassName = el.getAttribute('class') || '';
                    return this._classRegex(className).test(currentClassName);
                },
                remove: function removeCSSClass(el, className){
                    var currentClassName = el.getAttribute('class') || '';
                    el.setAttribute('class', currentClassName.replace(this._classRegex(className), ' '));
                },
                add: function addCSSClass(el, className){
                    if(this.contains(el, className)) return;
                    var currentClassName = el.getAttribute('class') || '';
                    el.setAttribute('class', currentClassName + ' ' + className);
                },
                toggle: function toggleCSSClass(el, className){
                    var funcName = (this.contains(el, className) ? 'remove':'add')
                    this[funcName](el, className);
                }
            };
            // let css know we have js
            ns.cssClass.remove(doc.body, 'no-js');
            ns.loop = function _loop(items, func){
                var i = items.length;
                for(;i--;) func.apply(items, [items[i], i]);
            };
            ns.every = function _loop(items, func){
                var i = items.length;
                for(;i--;) if(!func.apply(items, [items[i], i])) return false;
                return true;
            };
            ns.map = function _loop(items, func){
                var l = items.length, i = 0, returnArray = [];
                for(;i < l;) returnArray.push(func.apply(items, [items[i], i++]));
                return returnArray;
            };
            ns.getScroll = function(){
                if(win.pageYOffset !== undefined) {
                    return {'x': win.pageXOffset, 'y': win.pageYOffset};
                }
                else {
                    var x, y, d = document, r = d.documentElement, b = d.body;
                    x = r.scrollLeft || b.scrollLeft || 0;
                    y = r.scrollTop  || b.scrollTop  || 0;
                    return {'x': x, 'y': y};
                }
            };
            ns.hasTransitionEndSupport = (function _transitionEnd() {
                var el = document.createElement('div');
                var transEndEventNames = {
                    WebkitTransition : 'webkitTransitionEnd',
                    MozTransition    : 'transitionend',
                    OTransition      : 'oTransitionEnd otransitionend',
                    transition       : 'transitionend'
                };
                for (var name in transEndEventNames) { if(el.style[name] !== undefined) return transEndEventNames[name]; }
                return false;
            })();
            ns.cookie = {
                write: function writeCookie(name, value, days) {
                    var d = new Date;
                    d.setTime(d.getTime() + 24*60*60*1e3*days);
                    document.cookie = name + "=" + value + ";path=/;expires=" + d.toGMTString();
                }
            };
            ns.b2s = function _bool2string(bool){
                return bool ? 'true':'false';
            };
            ns.filter = function _filter(elements, filterFn) {
                var out = [];
                for (var i = elements.length; i--;) {
                    if (filterFn(elements[i])) out.unshift(elements[i]);
                }
                return out;
            };
            ns.array = {
                indexOf: function(searchElement, fromIndex) {
                    //https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Array/indexOf#Polyfill
                    var k;
                    if(this == null) throw new TypeError('"this" is null or not defined');
                    var O = Object(this);
                    var len = O.length >>> 0;
                    if (len === 0) return -1;
                    var n = +fromIndex || 0;
                    if (Math.abs(n) === Infinity) n = 0;
                    if (n >= len) return -1;
                    k = Math.max(n >= 0 ? n : len - Math.abs(n), 0);
                    while (k < len) {
                        if (k in O && O[k] === searchElement) return k;
                        k++;
                    }
                    return -1;
                }
            };
            ns.debounce = function _debounce(func, threshold, execAsap){
                threshold = threshold || 100;
                if(execAsap !== true) execAsap = false;
                var timeout;
                return function debounced(){
                    var obj = this, args = arguments;
                    function delayed(){
                        if(!execAsap)func.apply(obj, args);
                        timeout = null;
                    }
                    if(timeout)clearTimeout(timeout);
                    else if(execAsap)func.apply(obj, args);
                    timeout = setTimeout(delayed, threshold || 100);
                }
            };
            ns.loadScript = function _loadScript(url, condition, callback){
                var tries = 0, maxTries = 10, el = doc.createElement('script');
                el.src = url;
                doc.getElementsByTagName('head')[0].appendChild(el);
                if( ! condition) return;
                function run(timeout){ setTimeout(init, timeout); }
                function init(){
                    if(condition.call()) return callback.call();
                    if(tries++ < maxTries) run(50);
                }
                run(100);
            };
            ns.pubsub = (function _pubSub(){
                var topics = {};
                var hOP = topics.hasOwnProperty;
                return {
                    subscribe: function(topic, listener) {
                        if(!hOP.call(topics, topic)) topics[topic] = [];
                        var index = topics[topic].push(listener) -1;
                        return {
                            remove: function() {
                                delete topics[topic][index];
                            }
                        };
                    },
                    publish: function(topic, info) {
                        if(!hOP.call(topics, topic)) return;
                        var i = topics[topic].length;
                        for(;i--;)(function(item) {
                            item(info != undefined ? info : {});
                        })(topics[topic][i]);
                    }
                };
            })();
