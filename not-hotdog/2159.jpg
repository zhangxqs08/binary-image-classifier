<script>
var animating = false;
var dropDownLeft = 0;
function dropDownContainerSetPosition() {
	var left = dropDownLeft;
	var dropDownWidth = jQuery('.menuRow').position().left + jQuery('.menuRow').width() - left;
	jQuery('.dropDownContainerInner').css('width',dropDownWidth);
	var top = jQuery('.menuRow').position().top + 54;
	jQuery('.dropDownContainer').css('left', left);
	jQuery('.dropDownContainer').css('top', top);
}

function liToDiv(target, rows) {
	var i = 0;
	var j = 0;
	var currentDiv = jQuery('<div></div>');
	currentDiv.addClass('locationVisible');
	currentDiv.attr('locationGroup',0);
	currentDiv.appendTo(target);
	jQuery(target + ' a').each(function() {
		var newButton = jQuery('<div class="menuButton"></div>');
		newButton.text(jQuery(this).text());
		newButton.attr('href', jQuery(this).attr('href'));
		newButton.click(divLinkClick);
		newButton.appendTo(currentDiv);
		i++;
		if (i > (rows - 1)) {
			j++;
			currentDiv = jQuery('<div></div>');
			if (Math.floor(j/3) == 0) currentDiv.addClass('locationVisible');
			currentDiv.attr('locationGroup',Math.floor(j/3));
			currentDiv.appendTo(target);
			i = 0;
		}
	});
	jQuery(target + ' a').remove();
}

jQuery(function() {
	dropDownContainerSetPosition();
	jQuery( window ).resize(dropDownContainerSetPosition);
	
	liToDiv('.locationLinks', 4);
	liToDiv('.oacLinks', 2);
});
function searchToggle(animate) {
	if (homepage) {
		if (animate != false)
			performSearch();
		return false;
	}
	jQuery('.dropDownContainerInner').hide();
	if (typeof animate == 'undefined') animate = true;
	if (!jQuery(this).parent().hasClass('searchClose') &&
		jQuery('.searchSlide input').val().length > 0 &&
		jQuery('.searchSlide input:visible').length > 0 &&
		animate) {
		performSearch();
		return false;
	}
	if (animating) return false;
	animating = true;
	if (jQuery('.searchClose:visible').length > 0) {
		jQuery('.searchClose').css('display','none');
	} else {
		jQuery('.searchClose').css('display','table-cell');
	}
	if (jQuery('.subMenu').width() > 1) { animating = false; menuToggle(false); }
	if (animate)
		jQuery('.searchSlide input').toggle("slide", { direction: "right" }, 300, function() {animating = false});
	else {
		jQuery('.searchSlide input').toggle();
		animating = false
	}
	if (jQuery('.searchSlide input:visible').length > 0 && animate != false) jQuery('.searchSlide input').focus();
}
menuToggling = false;
function menuToggle(animate) {
	menuToggling = true;
	if (typeof animate == 'undefined') animate = true;
	//jQuery('.locationsLeftArrow, .locationsRightArrow').hide();
	if (animating) return false;
	animating = true;
	if (jQuery('.menuClose:visible').length > 0) {
		jQuery('.menuClose').css('display','none');
	} else {
		jQuery('.menuClose').css('display','table-cell');
	}
	
	if (jQuery('.searchSlide input:visible').length > 0) { animating = false; searchToggle(false); }
	
	var opening = true;
	if (jQuery('.subMenu').width() > 1) opening = false;
	
	if (opening) jQuery('.menuToggle').css({'max-width': 0});
	
	if (animate) {
		if (!opening) {
			jQuery('.menuToggle').delay(300).animate({'max-width': 1000},300);
			jQuery('.subMenu').animate({'max-width': 0},600, function() { animating = false; });
		}
		else
			jQuery('.subMenu').animate({'max-width': 1000},500, function() { animating = false; });
	} else {
		if (jQuery('.subMenu').width() > 1) {
			jQuery('.subMenu').css({'max-width': 0});
			jQuery('.menuToggle').css({'max-width': 1000});
		} else {
			jQuery('.subMenu').css({'max-width': 1000});
			jQuery('.menuToggle').css({'max-width': 0});
		}
		animating = false
	}
}
function dropDownToggle() {
	if(animating) return false;
	dropDownContainerSetPosition();
	animating = true;
	jQuery('.dropDownContainerInner').slideToggle(function() { animating = false; });
}
function locationsMoveRight() {
	var i = parseInt(jQuery('.locationLinksContainer').attr('currentPage'));
	i = i + 1;
	locationMove(i);
}
function locationsMoveLeft() {
	var i = parseInt(jQuery('.locationLinksContainer').attr('currentPage'));
	i = i - 1;
	locationMove(i);
}
function locationMove(i) {
	maxLocation = 0;
	jQuery('.locationLinksContainer > div > div').each(function() {
		if (parseInt(jQuery(this).attr('locationGroup')) > maxLocation) maxLocation = jQuery(this).attr('locationGroup');
	});
	if (i < 0 || i > maxLocation) return false;
	
	jQuery('.locationLinksContainer').attr('currentPage',i);
	
	jQuery('.locationVisible').removeClass('locationVisible');
	
	jQuery('.locationLinksContainer > div > div').each(function() {
		if (parseInt(jQuery(this).attr('locationGroup')) == i) jQuery(this).addClass('locationVisible');
	});
	
	if (i == 0)
		jQuery('.locationsLeftArrow img').css('opacity',0.2);
	else
		jQuery('.locationsLeftArrow img').css('opacity',1);
		
	if (i == maxLocation)
		jQuery('.locationsRightArrow img').css('opacity',0.2);
	else
		jQuery('.locationsRightArrow img').css('opacity',1);
}
locationsHover = false;
function dropDownMouseIn() {
	if (menuToggling) {
		if (!animating) menuToggling = false;
		else return false;
	}
	locationsHover = true;
	dropdown = null;
	if (typeof jQuery(this).attr('dropdown') == 'undefined') {
		jQuery('.dropDownContainerInner > div').each(function() {
			if (jQuery(this).css('display') != 'none') { dropdown = jQuery(this).attr('dropdown');
			}
		});
	} else dropdown = jQuery(this).attr('dropdown');
	dropDownLeft = jQuery("div[dropdown='" + dropdown + "']").position().left;
	dropDownContainerSetPosition();
	dropDownOpen(dropdown);
}
function dropdownMouseOut() {
	locationsHover = false;
	setTimeout(dropDownClose, 500);
}
function dropDownOpen(target) {
	if (target.endsWith('DropDown')) {
		
	}
	if (!locationsHover) return false;
	jQuery('.dropDownContainerInner > div').each(function() {
		if (jQuery(this).hasClass(target)) jQuery(this).show();
		else jQuery(this).hide();
	});
	if (jQuery('.dropDownContainerInner:visible').length == 0) dropDownToggle();
	//else setTimeout(dropDownOpen, 250, target);
}

function dropDownClose() {
	if (locationsHover) return false;
	if (jQuery('.dropDownContainerInner:visible').length > 0) dropDownToggle();
	//else setTimeout(dropDownClose, 250);
}

function divLinkClick() {
	window.location.href = jQuery(this).attr('href');
	return true;
}

var homepage = false;
function homepageHeader() {
	jQuery('.subMenu').css({'max-width': 1000});
	jQuery('.menuToggle').css({'max-width': 0});
	jQuery('.searchSlide').detach().insertBefore('.dropDownContainer');
	jQuery('.searchSlide').css('float','right');
	jQuery('.searchSlide input').show();
	jQuery('.menuSearchBox').detach().appendTo('.searchSlide');
	jQuery('.menuSearchBox').css('display','inline-block');
	jQuery('.menuSearchBox').css('padding','15px');
	jQuery('#search-text').css('width','624px');
	homepage = true;
}

function mobileMenuToggle() {
	if (jQuery('#mobileMenu:visible').length == 0)
		jQuery('#mobileMenuButton img').attr('src','/Style Library/LINC/CSS/images/menuClose.png');
	else
		jQuery('#mobileMenuButton img').attr('src','/Style Library/LINC/CSS/images/menuButton.png');

	jQuery('#mobileMenu').slideToggle();
}

jQuery('div[href]').click(divLinkClick);
jQuery('.menuSearchBox').click(searchToggle);
jQuery('.searchClose div').click(searchToggle);
jQuery('.menuToggle').click(menuToggle);
jQuery('.menuClose div').click(menuToggle);
jQuery('.locationsRightArrow').click(locationsMoveRight);
jQuery('.locationsLeftArrow').click(locationsMoveLeft);
jQuery('div[dropdown]').hover(dropDownMouseIn, dropdownMouseOut);
jQuery('.dropDownContainer').hover(dropDownMouseIn, dropdownMouseOut);
jQuery('#mobileMenuButton').click(mobileMenuToggle);

jQuery(function() {
	jQuery('.subMenu > div > div').each(function() {
		jQuery('#mobileMenu').append('<div class="mobileMenuButton"><a href="' + jQuery(this).attr('href') + '">' + jQuery(this).text() + '</a></div>');
	});
	jQuery('#mobileMenu').append('<div class="mobileMenuButton">' + jQuery('.myLibrary').html() + '</div>');
});

jQuery(function() {
	if (jQuery('body').width() < 400) {
		homepageHeader();
	}
	searchToggle(false);
});
</script>

						
