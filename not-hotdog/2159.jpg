	<style>
	
	#MainSlideshow>div{position:absolute;}
	
	</style>
	
	<script>
	var currentSlide = 1;
	var slideShowAnimating = true;
	$(document).ready(function(){
	
		$('#MainSlideshow > div').each(function() {
			var slideNo = $(this).attr('slideNo');
			var img = $('<img>');
			img.click(function() {selectSlide(slideNo);slideShowAnimating = false;});
			if (slideNo == 1)
				img.attr('src','/SiteAssets/dot-filled-white@1x.png');
			else
				img.attr('src','/SiteAssets/dot-unfilled-inactive-white@1x.png');
			img.css('margin-left','20px');
			img.attr('id','slideShowButton' + slideNo);
			img.addClass('slideShowButton');
			img.appendTo('#slideShowButtons');
		});
		
		$("#MainSlideshow> div:gt(0)").hide();
	
	setInterval(function() {
		if (!slideShowAnimating) return false;
		currentSlide++;
		if ($("[slideNo='" + currentSlide + "']").length == 0) currentSlide = 1;
		selectSlide(currentSlide);
	},  7500);
	
	});

	function selectSlide(i) {
		if ($("[slideNo='" + i + "']").length == 0) return false;
		currentSlide = i;
		$('.slideShowButton').attr('src','/SiteAssets/dot-unfilled-inactive-white@1x.png');
		$('#slideShowButton' + i).attr('src','/SiteAssets/dot-filled-white@1x.png');
		$('#MainSlideshow> div:visible').fadeOut(1000);
		$("[slideNo='" + i + "']").fadeIn(1000);
	}
	
	</script>
			

