<script type="text/javascript">
function onSignInDialogSubmit()
{
	var required_elements = document.getElementById('sign_in_modal').getElementsByClassName("required");

	for ( var i = 0; i < required_elements.length; i++ )
	{
		if (!required_elements[i].value || required_elements[i].value == null || required_elements[i].value.length == 0)
		{
			document.getElementById("prevent_close_sign_in").className = document.getElementById("prevent_close_sign_in").className.replace("invisible_text", "");
			document.getElementById("prevent_close_sign_in_content").innerHTML = "Please fill out all required fields.";
			return;
		}
	}

	var form = createElement("form");
	form.appendChild(createElement('input', {'name':'email', 'value':document.getElementById("SIGN_IN_MODAL_EMAIL").value}));
	form.appendChild(createElement('input', {'name':'password', 'value':document.getElementById("SIGN_IN_MODAL_PASSWORD").value}));

	var data = new FormData(form);
	rwsAJAXPostFormAJAXWithWaitFade('/profile/log-in-account', data, onResponse);

	function onResponse(response)
	{
		if (response.success)
		{
			document.getElementById("sign_in_content_confirmation").className = document.getElementById("sign_in_content_confirmation").className.replace("invisible_text", "");
			document.getElementById("sign_in_content").className += " invisible_text";

			document.getElementById("consumer_name").innerHTML = response.name;

			if (dialog) dialog.close();
		}
		else
		{
			document.getElementById("warning_container").className = document.getElementById("warning_container").className.replace("invisible_text", "")
		}
	}
}

function goBackOrSendToIndexPage()
{
    var last_url = document.referrer;

	// if the last url navigates them away from the site, send them to the index page
	if ( last_url && last_url.indexOf('www.furniturestorenc.com') == -1 )
	{
		document.location = 'www.furniturestorenc.com';
	}

	window.history.back();
}
</script>
			

