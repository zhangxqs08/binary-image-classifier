<script type="text/javascript">
function onAuthenticationDialogSubmit()
{
	var required_elements = document.getElementById('authentication_modal').getElementsByClassName("required");

	for ( var i = 0; i < required_elements.length; i++ )
	{
		if (!required_elements[i].value || required_elements[i].value == null || required_elements[i].value.length == 0)
		{
			document.getElementById("prevent_close_authenticate").className = document.getElementById("prevent_close_authenticate").className.replace("invisible_text", "");
			document.getElementById("prevent_close_authenticate_content").innerHTML = "Please fill out all required fields."
			return;
		}
	}
	
	if ( authenticationPasswordTest() == false )
	{
		if ( document.getElementById("AUTHENTICATION_MODAL_PASSWORD").value.length < 8 ) 
		{
			alert("Password must be at least 8 characters.")
		}
		else
		{
			alert("Passwords do not match.");
		}
		return;
	}
	var form = document.getElementById("profile_authentication");
	if ( form == null ) return;

	form.appendChild(createElement('input', {'class':'invisible_text', 'name':'wfid', 'value':'31971'}));

	var data = new FormData(form);

	// VALIDATE ON CHECKBOX
	if ( !document.getElementById("AUTHENTICATION_MODAL_OPT_IN").checked )
	{
		document.getElementById("prevent_close_authenticate").className = document.getElementById("prevent_close_authenticate").className.replace("invisible_text", "");
		document.getElementById("prevent_close_authenticate_content").innerHTML = "Please acknowledge that you agree to the Terms of Service."
		return;
	}

	rwsAJAXPostFormAJAXWithWaitFade('/profile/authenticate-account', data, onResponse);

	function onResponse(response)
	{
		if (response.success)
		{
			alert(response.message);
			if (dialog) dialog.close();
		}
		else
		{
			alert("Error: " + response.error_msg);
		}
	}
}

function goBackOrSendToIndexPage()
{
    var last_url = document.referrer;

	// if the last url navigates them away from the site, send them to the index page
	if ( last_url && last_url.indexOf('www.furniturestorenc.com') == -1 )
	{
		document.location = 'www.furniturestorenc.com';
	}

	window.history.back();
}

function authenticationPasswordTest()
{
	// Check if password fields are identical
	var pass1 = document.getElementById("AUTHENTICATION_MODAL_PASSWORD");
	var pass2 = document.getElementById("AUTHENTICATION_MODAL_PASSWORD_CONFIRM");

	var warning = document.getElementById("authentication_password_match_warning");

	if ( pass1.value.length < 8 || pass2.value.length <= 0 )
	{
		if ( warning.className.indexOf('invisible_text') > 0 )
		{
			warning.className = warning.className.replace("invisible_text","");
		}
		warning.innerHTML = "&nbsp;";

		return false;
	}

	if ( pass1.value != pass2.value )
	{
		if ( warning.className.indexOf('invisible_text') > 0 )
		{
			warning.className = warning.className.replace("invisible_text","");
		}
		warning.innerHTML = "Passwords do not match";

		return false;
	}
	
	

	warning.innerHTML = "&nbsp;";

	return true;
}
