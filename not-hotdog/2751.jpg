<script type="text/javascript">

function switch_liked( id ){

	item = $("a[rel="+id+"]");
	item.removeClass("like").addClass("liked");

	item = $(".bigdot[rel="+id+"]");
	html = item.html();
	if (html) {
		html = html.replace("Like ","Liked ");
		item.html(html);
		item.removeClass("biglike").addClass("bigliked");
	}
}	

function update_like_count( id,count ){
	item = $("[rel="+id+"]");
	item.find("span").html(count);
}

function get_cookie ( cookie_name ) {
  var results = document.cookie.match ( "(^|;) ?" + cookie_name + "=([^;]*)(;|$)" );

  if ( results )
    return ( unescape ( results[2] ) );
  else
    return null;
}
var mocolikeinit = (function($) {
	var mylikes = get_cookie ("mocolikes");
	if (mylikes!=null) {
		var theIDs = mylikes.split(",");
	}
	$(".like").each(function() {
		var id = $(this).attr("rel");
		var item = $("a[rel="+id+"]");
		if(theIDs!=null && $.inArray(id, theIDs)!=-1) {
			//item.removeClass("like").addClass("liked");	
			switch_liked(id);		
		}			
	});
	$(".bigdot").each(function() {
		var id = $(this).attr("rel");
		var item = $(this);
		if(theIDs!=null && $.inArray(id, theIDs)!=-1) {
			//html = item.html();
			//html = html.replace("Like","Liked");
			//item.html(html);
			switch_liked(id);		
		}			
	})
	
	$(".like").click(function() {
		var id = $(this).attr("rel");
		var data = {
			action: "m_like_action",
			postid: id
		};
		var item = $("a[rel="+id+"]");
		$.post("https://mocoloco.com/wp-admin/admin-ajax.php", data, function(response) {
			//item.removeClass("like").addClass("liked");	
			switch_liked(id);			
			if(response>0 && response<10) item.addClass("med");
			if(response>9) { item.addClass("big"); }
			//item.find("span").html(response);
			update_like_count(id, response);
		});
		$(this).unbind("click");
	});
	$(".bigdot.biglike").click(function() {
		var id = $(this).attr("rel");
		var data = {
			action: "m_like_action",
			postid: id
		};
		var item = $(this);
		$.post("https://mocoloco.com/wp-admin/admin-ajax.php", data, function(response) {
			//html = item.html();
			//html = html.replace("Like","Liked");
			//item.html(html)	
			switch_liked(id);		
			if(response>0 && response<10) item.addClass("med");
			if(response>9) { item.addClass("big"); }
			//item.find("span").html(response);
			update_like_count(id, response);
		});
		$(this).unbind("click");
	});

});

(function(){
 var loopback = arguments.callee;
 if(!window.jQuery){
  setTimeout(loopback, 1000);
 }else{
   mocolikeinit(jQuery);
 }
})();
</script><script type="text/javascript">var moco_likes_post_url = "https://mocoloco.com/wp-admin/admin-ajax.php"</script>


