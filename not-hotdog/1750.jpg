function callBasketCountHandler(language, action) {
	var xwtObj = new XWTObject();
	xwtObj.setParameter('AJAX_HANDLER', 'basket');
	xwtObj.setParameter('action', action);
	xwtObj.setParameter('language', language);
	var callback = new BasketCountCallback();
	xwtObj.setCallbackHandler(callback);
	var path = '/AJAXControllerServlet';
	xwtObj.submit(path + '?' + xwtObj.getQuery(xwtObj));
}
function BasketCountCallback() {
	this.callback = function(reqResponseXML) {
		var countNode = reqResponseXML.getElementsByTagName("count")[0];
		if (countNode) {
			var count = countNode.childNodes[0].nodeValue;
			document.getElementById("header_myBasketCount").innerHTML = count;
		}
	}
}
function ssoCheckCallback(){
	var currLocale = document.getElementById("locale").value;
	callBasketCountHandler(currLocale,'getMyBasketCount');
	getSSOStatus();
}
</script>

