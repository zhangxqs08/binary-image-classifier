     function checkDup(keywordstr, kword) {
		var keywordList = keywordstr.split(',');
		var index = -1;
		for (var i = 0; i < keywordList.length; i++) {
			var keyword = keywordList[i];
			if (keyword == kword) {
				index = i;
				break;
			}
		}
		if (i >= keywordList.length)
		{
			keywordList[keywordList.length] = kword;
		}
		var keywords = join_q(keywordList, ',');
		return keywords;
	}     
	function join_q(idsValues, str) {
		var ret = '';
		var i = 0;
		if (idsValues.length > 3)
		{
			i = 1;
		} else {
			i = 0;
		}
		for (i; i < idsValues.length && i <= 4; i++) {
			var idValue = idsValues[i];
			if (idValue != '') {
				ret += idValue;
				if (i != idsValues.length - 1) {
					ret += str;
				}
			}
		}
		return ret;
	}     
	
